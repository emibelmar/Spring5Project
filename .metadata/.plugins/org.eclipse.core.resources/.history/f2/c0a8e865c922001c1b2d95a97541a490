package com.ebmdev.sprinboot.app;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;
import org.springframework.context.annotation.Primary;

import com.ebmdev.sprinboot.app.services.IndexService;
import com.ebmdev.sprinboot.app.services.IndexServiceImpl;
import com.ebmdev.sprinboot.app.services.IndexServiceImpl2;

@Configuration
public class AppConfig {

	public IndexService indexService;

	@Bean("IndexServiceImpl")
	public IndexService registrarIndexServiceImpl() {
		return new IndexServiceImpl();
	}
	
	@Primary
	@Bean("IndexServiceImpl2")
	public IndexService registrarIndexServiceImpl2() {
		return new IndexServiceImpl2();
	}
}
